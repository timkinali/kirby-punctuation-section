{
  "name": "kirby-punctuation-section",
  "type": "module",
  "version": "3.0.12",
  "private": true,
  "packageManager": "pnpm@8.15.1",
  "scripts": {
    "dev-watch": "npx -y kirbyup src/panel/index.js --watch",
    "dev": "npx -y kirbyup serve src/panel/index.js",
    "build": "npx -y kirbyup src/panel/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"src/panel/**\" --write",
    "release": "pnpm run build && bumpp --all"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@unocss/eslint-plugin": "^0.58.4",
    "autoprefixer": "^10.4.17",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "kirbyup": "^3.1.3",
    "prettier": "^3.2.5"
  },
  "optionalDependencies": {
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1"
  }
}
